{
    "name": "Hull.js",
    "version": "0.2.0",
    "description": "JavaScript library that builds concave hulls (shapes) by set of points",
    "homepage": "https://github.com/AndreyGeonya/hull",
    "keywords": [
        "geometry",
        "concave-hull",
        "math",
        "alpha-shape",
        "algorithm"
    ],
    "author": "Andrey Geonya <a.geonya@gmail.com>",
    "repository": {
        "type": "git",
        "url": "https://github.com/AndreyGeonya/hull"
    },
    "main": "./src/hull.js",
    "devDependencies": {
        "mocha": "~1.18.2",
        "jshint": "~2.5.0",
        "browserify": "~5.11.0"
    },
    "scripts": {
        "test": "./node_modules/jshint/bin/jshint src/intersect.js && ./node_modules/jshint/bin/jshint src/grid.js && ./node_modules/jshint/bin/jshint src/hull.js && ./node_modules/.bin/mocha -R spec && ./node_modules/browserify/bin/cmd.js ./src/hull.js --standalone hull > ./dist/hull.js"
    },
    "jshintConfig": {
        "forin": false,
        "node": true
    },
    "license": "BSD"
}